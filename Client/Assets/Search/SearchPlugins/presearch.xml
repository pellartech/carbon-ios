<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Presearch</ShortName>
<Description>Presearch is a decentralized search engine, powered by the community.</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="32" height="32" type="image/svg+xml"></Image>
<Url type="text/html" method="get" template="https://presearch.com/search?q={searchTerms}"/>
<Url type="application/x-suggestions+json" method="get" template="https://presearch.com/api/suggest?q={searchTerms}"/>
<SearchForm>https://presearch.com</SearchForm>
</SearchPlugin>
